@@ -0,0 +1,37 @@
+# Build with a compiler that offers standard parallel algorithm offload, with the latter turned on.
+
+SHELL = /bin/sh
+.SUFFIXES: .cc .o
+
+LULESH_EXEC = lulesh2.0
+
+CXX = ${STDPAR_CXX}
+
+SOURCES2.0 = \
+	lulesh.cc \
+	lulesh-comm.cc \
+	lulesh-viz.cc \
+	lulesh-util.cc \
+	lulesh-init.cc
+OBJECTS2.0 = $(SOURCES2.0:.cc=.o)
+
+CXXFLAGS = -O3 -stdpar --stdpar-path=${STDPAR_PATH} -std=c++17 -DUSE_MPI=0 -DHIP_STDPAR --offload-arch=${STDPAR_TARGET}
+LDFLAGS = -stdpar -ltbb
+
+%.o: ../src/%.cc ../src/lulesh.h
+	@echo "Building $<"
+	$(CXX) -c $(CXXFLAGS) -o $@  $<
+
+all: $(LULESH_EXEC)
+
+$(LULESH_EXEC): $(OBJECTS2.0)
+	@echo "Linking"
+	$(CXX) $(OBJECTS2.0) $(LDFLAGS) -lm -o $@
+
+run: $(LULESH_EXEC)
+	./lulesh2.0 -p -s 150
+
+clean:
+	/bin/rm -f *.o *~ $(OBJECTS) $(LULESH_EXEC)
+	/bin/rm -rf *.dSYM
+
